#include <iostream>
#include <fstream>
#include <string>
using namespace std;
int main()
{
    fstream fl;
    fl.open("flita.txt");
    int n;
    cin >> n;
    int** arr = new int* [n];
    for (int i = 0; i < n; i++)
    {
        arr[i] = new int[n];
    }
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n; j++)
        {
            fl >> arr[i][j];
        }
    }
    fl.close();
    int a = 0;
    int b = 0;
    ofstream Fl;
    Fl.open("flita1.txt");
    if (Fl.is_open()) 
    {
        Fl << "graph {" << endl;
        for (int i = 0; i < n; i++)
        {
            for (int j = 0; j < n; j++)
            {
                if (arr[i][j] != 0)
                {
                    Fl << i + 1;
                    Fl << " -- ";
                    Fl << j + 1 << endl;
                    if (arr[i][j] > 1)
                    {
                        Fl << " [label = ";
                        Fl << arr[i][j];
                        Fl << "]" << endl;
                    }
                }
            }
        }
        Fl << "}";
    }
    return 0;
}
